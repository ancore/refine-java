                          }
                        },
                        "values": [
                          {
                            "valueSource": {
                              "source": "column",
                              "columnName": "City"
                            },
                            "valueType": {
                              "type": "literal"
                            }
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    "description": "Save RDF Mapping"
  },
  {
    "op": "core/text-transform",
    "engineConfig": {
      "facets": [],
      "mode": "row-based"
    },
    "columnName": "City",
    "expression": "value.toTitlecase()",
    "onError": "keep-original",
    "repeat": false,
    "repeatCount": 10,
    "description": "Text transform on cells in column City using expression value.toTitlecase()"
  }
]